<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="SuperDartBrosApp.WinnerUserControl">

	<Grid Background="#2E3B4E">
		<Label x:Name="LblWinner" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,50" Foreground="WhiteSmoke" Content="Spieler hat Gewonnen!"/>
		<Button x:Name="BtnNewGame" HorizontalAlignment="Center" Margin="0,30,0,0" Padding="15,7" FontSize="16" Background="#FF8C00" Foreground="White" BorderThickness="0" CornerRadius="5" Content="Neues Spiel starten!" Click="BtnNewGame_Click"/>
	</Grid>
	
</UserControl>
